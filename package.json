{
  "name": "uncle-toms-letters",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "env-cmd -f .env.production.local npm run deploy-s3",
