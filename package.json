{
  "name": "uncle-toms-letters",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "lint": "next lint",
    "dev": "next dev",
    "build": "next build",
    "start": "next start --port 3001",
    "export": "next export",
    "deploy": "env-cmd -f .env yarn deploy-s3",
    "deploy-s3": "aws s3 sync ./out ${UTL_S3_BUCKET} --exclude '.*'",
    "postdeploy-s3": "aws cloudfront create-invalidation --distribution-id ${UTL_DISTRIBUTION_ID} --paths '/*'",
    "upload": "env-cmd -f .env yarn upload-s3",
    "upload-s3": "aws s3 sync ./posts s3://${UTL_S3_REPOSITORY}/posts --exclude '*.DS_Store' --delete",
    "postupload-s3": "aws s3 sync ./public/assets s3://${UTL_S3_REPOSITORY}/public/assets --exclude '*.DS_Store' --delete",
    "download": "env-cmd -f .env yarn download-s3",
    "download-s3": "ts-node --esm ./scripts/aws.ts",
    "makewebp": "ts-node --esm ./scripts/webp.ts",
    "automate": "ts-node --esm ./scripts/auto.ts"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.19.0",
    "husky": "^6.0.0",
    "nanoid": "^3.1.23",
    "next": "^12.1.4",
    "next-plugin-preact": "^3.0.6",
    "preact": "^10.7.2",
    "preact-render-to-string": "^5.2.0",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat",
    "react-ssr-prepass": "npm:preact-ssr-prepass"
  },
  "devDependencies": {
    "@nosferatu500/textract": "^3.1.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/sharp": "^0.31.1",
    "@types/shelljs": "^0.8.11",
    "@types/yamljs": "^0.2.31",
    "autoprefixer": "^10.2.6",
    "aws-sdk": "^2.882.0",
    "babel-eslint": "^10.1.0",
    "browserslist": "^4.16.6",
    "cld": "^2.9.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-next": "12.1.6",
    "image-size": "^1.0.1",
    "lint-staged": "^12.4.1",
    "mkdirp": "^1.0.4",
    "postcss": "^8.3.0",
    "prettier": "^2.8.7",
    "sass": "^1.34.0",
    "sharp": "^0.30.4",
    "shelljs": "^0.8.5",
    "tailwindcss": "^2.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "yamljs": "^0.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "yarn lint",
    "*.{ts,tsx,js,jsx,css,scss,toml,md,json,yml}": "prettier --write"
  }
}
